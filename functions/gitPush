function gitPush() {
    while [ "$1" != "" ]; do
    case $1 in
    -h | -help)
      _showHelp "$0"
      return 0
      ;;
    *)
      echo "Invalid option"
      _showHelp "$0"
      return 0
      ;;
    esac
    shift
  done
  local CURRENT_BRANCH
  CURRENT_BRANCH=$(git branch --show-current)
  git push --set-upstream origin "$CURRENT_BRANCH"
}

_showHelp() {
  echo "Function help:"
  echo "This function runs:"
  echo "  git push --set-upstream origin $CURRENT_BRANCH "
  echo "$1 [options]"
  echo " "
  echo "options:"
  echo "-h | -help,       Show brief help"
}

gitPush "$@"